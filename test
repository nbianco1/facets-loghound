<script>
 function init() {
    adjustOnResize();
    window.onresize = function(event) {
      adjustOnResize();
    }
}
function adjustOnResize() {
  var screenDims = getAvailSize();
  var leftElem = document.getElementById('leftColumn');
  var rightElem = document.getElementById('rightColumn');
  var middleElem = document.getElementById('middleColumn');
  var columnWidth = (screenDims.width - 960)/2;
  if (leftElem) {
    leftElem.style.width = columnWidth + 'px';
  }
  if (rightElem) {
    rightElem.style.width = columnWidth + 'px';
  }
  if (middleElem) {
    middleElem.style.marginRight = columnWidth + 'px';
    middleElem.style.marginLeft = columnWidth + 'px';
  }
}
 </script>
